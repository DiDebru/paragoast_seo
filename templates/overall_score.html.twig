<div class="yoast-seo-score-widget">
    <label>{% trans %}SEO scores{% endtrans %}</label>
    <ul>
        <li {% if overall_score_target_id %}id="{{ overall_score_target_id }}"{% endif %}
            class="overallScore {{ overall_score }}">
            <label>{% trans %}Estimated{% endtrans %}<a class="help"></a></label>
            <span class="score_circle"></span>
            <span class="score_value">{{ overall_score }}</span>
        </li>
    </ul>
</div>
